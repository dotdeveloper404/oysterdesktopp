            <FontFamily x:Key="PoppinsRegular">pack://application:,,,/assets/fonts/poppins/Poppins-Regular.ttf#Poppins</FontFamily>
            <FontFamily x:Key="PoppinsSemiBold">pack://application:,,,/assets/fonts/poppins/Poppins-SemiBold.ttf#Poppins</FontFamily>
            <FontFamily x:Key="PoppinsThin">pack://application:,,,/assets/fonts/poppins/Poppins-Thin.ttf#Poppins</FontFamily>
            <FontFamily x:Key="PoppinsThinItalic">pack://application:,,,/assets/fonts/poppins/Poppins-ThinItalic.ttf#Poppins</FontFamily>

        </ResourceDictionary>
    </Application.Resources>


</Application>

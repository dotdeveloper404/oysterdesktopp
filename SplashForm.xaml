<Window x:Class="OysterVPN.SplashForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OysterVPN"
        UseLayoutRounding ="True"
         mc:Ignorable="d"
        Height="500" Width="880"
        WindowStartupLocation="CenterScreen" 
        FontFamily="Poppins" Visibility="Visible"  WindowStyle="None" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" ResizeMode="NoResize">

    <Window.Background>
        <ImageBrush ImageSource="assets/SplashScreen.png"/>
    </Window.Background>
  
</Window>
